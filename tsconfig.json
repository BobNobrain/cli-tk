{
    "compilerOptions": {
        "target": "ES2017",
        "module": "CommonJS",
        "checkJs": false,
        "declaration": true,
        "strict": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noEmitOnError": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}
